    <div class="collapse navbar-collapse navbar-inverse" id="bs-example-navbar-collapse-1">
      <ul class="nav navbar-nav">
        <li class="active"> <%= link_to "Page One", pages_one_url %> </li>
        <li> <%= link_to "Page Two", pages_two_url %> </li>
        <li> <%= link_to "Page Three", pages_three_url %></li>
      </ul>
    </div><!-- /.navbar-collapse -->
  </div><!-- /.container-fluid -->
</nav>

    <%= yield %>
